port ${VPNPORT}
proto ${VPNPROTO}
dev tun
topology subnet
ca ${VPNCAFILE}
cert ${VPNCERTFILE}
key ${VPNKEYFILE}
dh ${VPNDHFILE}
server 10.8.0.0 255.255.255.0
push "redirect-gateway def1 bypass-dhcp"
push "dhcp-option DNS 8.8.8.8"
push "dhcp-option DNS 1.1.1.1"
duplicate-cn
keepalive 10 120
cipher AES-256-GCM
data-ciphers AES-256-GCM:AES-256-CBC
auth SHA512
persist-key
persist-tun
status openvpn-status.log
verb 1
tls-server
tls-version-min 1.2
tls-auth ${VPNTAKEYFILE} 0
